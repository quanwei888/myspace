#include<stdio.h>
#include<stdlib.h> 
#include <sys/time.h>

#define LEN 1000000
#define R(x) (rand() % x)

int A[LEN];
int B[LEN]; 

void initData(int rc) {
    int i;

    int pos[rc];
    int base = 0;

    base = 0;
    for (i=0;i<rc;i++) {
        base += R((LEN - 1 )/rc) + 1;
        pos[i] = base;
    }

    int j = 0;
    base = 0;
    for (i=0;i<LEN;i++) {
        base += R(100);
        A[i] = base;

        if ( j < rc && i == pos[j]) {
            B[i] = base;
            j++; 
        } else {
            base += R(100);
            B[i] = base;
        }
    }
}
